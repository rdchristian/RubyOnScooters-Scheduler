- model_class = Obj_search

.page-header
	%h1=t '.title', :default => model_class.model_name.human.pluralize.titleize
%p
	%br
	= @obj_search.inspect
	%br
	= @obj_search.events_results
%table.table.table-striped
	%thead
		%tr
			%th= Event.human_attribute_name(:id)
			%th= Event.human_attribute_name(:title)
			%th= Event.human_attribute_name(:start_date)
			%th= Event.human_attribute_name(:start)
			%th= Event.human_attribute_name(:ending)
		%tbody
			- @obj_search.events_results
			- @obj_search.events_results.each do |eve|
				%tr
					%td= link_to event.id, user_event_path(event.creator, event)
					-if event.is_approved?
						%td= event.title
					-else
						%td= event.title+ '*'
					%td= event.start_date
					%td= event.start
					%td= event.ending